<status>
  <worker id='__machination__'>
    <directories
        cache='{dsdir}'
        />
    <logging>
      <stream id='stderr' loglevel='6'/>
      <file id="log.txt" loglevel="6"/>
    </logging>
    <daemon address="" port="1313" sleeptime="10000" />
    <openssl binary="C:\OpenSSL-Win64\bin\openssl.exe"
             config="C:\OpenSSL-Win64\bin\openssl.cfg">
    </openssl>
    <services>
      <service id="eng.ed.ac.uk">
        <hierarchy id="https://machination2.see.ed.ac.uk/machination/hierarchy"/>
        <authentication id="person"
                        type="cosign"
                        cosignLoginPage="https://www.ease.ed.ac.uk/"/>
      </service>
      <service id="localhost">
        <hierarchy id="https://localhost/machination/hierarchy"/>
        <authentication id="person"
                        type="cosign"
                        cosignLoginPage="https://www.ease.ed.ac.uk/"/>
      </service>
    </services>
  </worker>
</status>
